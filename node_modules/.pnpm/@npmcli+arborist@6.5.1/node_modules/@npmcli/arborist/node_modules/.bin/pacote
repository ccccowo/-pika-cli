#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules/pacote/lib/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules/pacote/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules/@npmcli/arborist/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules/@npmcli/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules/pacote/lib/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules/pacote/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/pacote@15.2.0/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules/@npmcli/arborist/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules/@npmcli/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@npmcli+arborist@6.5.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pacote@15.2.0/node_modules/pacote/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../pacote@15.2.0/node_modules/pacote/lib/bin.js" "$@"
fi
