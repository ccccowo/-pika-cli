#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/packages/cli/dist/node_modules:/Users/chensihong02/Desktop/cc-cli/packages/cli/node_modules:/Users/chensihong02/Desktop/cc-cli/packages/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/packages/cli/dist/node_modules:/Users/chensihong02/Desktop/cc-cli/packages/cli/node_modules:/Users/chensihong02/Desktop/cc-cli/packages/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@pika-cli/cli/dist/index.js" "$@"
else
  exec node  "$basedir/../@pika-cli/cli/dist/index.js" "$@"
fi
