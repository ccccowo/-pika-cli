#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/os-name@1.0.3/node_modules/os-name/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/os-name@1.0.3/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules/os-name/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/os-name@1.0.3/node_modules/os-name/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/os-name@1.0.3/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules/os-name/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../os-name/cli.js" "$@"
else
  exec node  "$basedir/../os-name/cli.js" "$@"
fi
