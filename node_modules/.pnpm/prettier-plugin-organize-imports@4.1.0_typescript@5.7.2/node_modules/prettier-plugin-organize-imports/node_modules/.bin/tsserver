#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/bin/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-organize-imports@4.1.0_typescript@5.7.2/node_modules/prettier-plugin-organize-imports/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-organize-imports@4.1.0_typescript@5.7.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/bin/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules/typescript/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/typescript@5.7.2/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-organize-imports@4.1.0_typescript@5.7.2/node_modules/prettier-plugin-organize-imports/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-organize-imports@4.1.0_typescript@5.7.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.7.2/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.7.2/node_modules/typescript/bin/tsserver" "$@"
fi
