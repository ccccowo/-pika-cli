#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/sort-package-json@2.12.0/node_modules/sort-package-json/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/sort-package-json@2.12.0/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-packagejson@2.5.6/node_modules/prettier-plugin-packagejson/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-packagejson@2.5.6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/sort-package-json@2.12.0/node_modules/sort-package-json/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/sort-package-json@2.12.0/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-packagejson@2.5.6/node_modules/prettier-plugin-packagejson/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/prettier-plugin-packagejson@2.5.6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sort-package-json@2.12.0/node_modules/sort-package-json/cli.js" "$@"
else
  exec node  "$basedir/../../../../../sort-package-json@2.12.0/node_modules/sort-package-json/cli.js" "$@"
fi
