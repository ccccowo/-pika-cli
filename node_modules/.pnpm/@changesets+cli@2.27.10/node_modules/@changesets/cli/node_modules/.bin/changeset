#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules/@changesets/cli/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules/@changesets/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules/@changesets/cli/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules/@changesets/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/@changesets+cli@2.27.10/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules/.pnpm/node_modules:/Users/chensihong02/Desktop/cc-cli/node_modules:/Users/chensihong02/Desktop/node_modules:/Users/chensihong02/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
